{
  "name": "webgis4u",
  "private": true,
  "version": "0.0.1-development",
  "main": "build/index.js",
  "bin": "scripts/cli.js",
  "scripts": {
    "doc:generate": "node_modules/.bin/jsdoc -c jsdoc.config.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "yarn test:coverage && cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js",
    "start": "webpack --watch --mode development",
    "start:all": "yarn start && lerna run start",
    "build": "yarn build:module && yarn build:templates",
    "build:module": "webpack -p",
    "build:templates": "yarn cli -out ./build/templates",
    "build:all": "yarn build && lerna run build",
    "templates:examples": "yarn cli -out ./build/templates_examples -templates ./src/html_fragment",
    "cli": "node ./scripts/cli.js",
    "lint": "eslint src/**",
    "prepublishOnly": "yarn build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/environment-agency-austria/webgis4u.git"
  },
  "license": "BSD-2-Clause",
  "bugs": {
    "url": "https://github.com/environment-agency-austria/webgis4u/issues"
  },
  "homepage": "https://github.com/environment-agency-austria/webgis4u#readme",
  "workspaces": [
    "packages/*"
  ],
  "sideEffects": false,
  "files": [
    "./build",
    "./scripts",
    ".pug"
  ],
  "devDependencies": {
    "@babel/core": "^7.1.2",
    "@babel/plugin-proposal-class-properties": "^7.2.3",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/polyfill": "^7.0.0",
    "@babel/preset-env": "^7.1.0",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^23.6.0",
    "babel-loader": "^8.0.4",
    "clean-webpack-plugin": "^1.0.0",
    "coveralls": "^3.0.2",
    "css-loader": "^2.0.1",
    "eslint": "^5.4.0",
    "eslint-config-airbnb": "^17.0.0",
    "eslint-config-oceanjs": "^0.1.0",
    "eslint-plugin-babel": "^5.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-react": "^7.11.1",
    "html-webpack-plugin": "^3.2.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^23.6.0",
    "jest-canvas-mock": "^1.1.0",
    "jsdoc": "^3.5.5",
    "lerna": "^3.9.0",
    "node-sass": "^4.11.0",
    "resolve-url-loader": "^3.0.0",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "url-loader": "^1.1.2",
    "webpack": "^4.23.1",
    "webpack-cli": "^3.1.2"
  },
  "dependencies": {
    "ol": "^5.3.0",
    "proj4": "^2.5.0"
  }
}
